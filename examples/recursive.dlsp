; Compute the lengh of a q-expression.
(fn {len l} {
  if (== l {})
    {0}
    {+ 1 (len (tail l))}
})

; Reverse the content of a q-expression.
(fn {reverse l} {
  if (== l {})
    {{}}
    {join (reverse (tail l)) (head l)}
})

(def {array} { 1 2 3 4 5 })

(print (len array))
(print (reverse array))