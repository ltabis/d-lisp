(recipe {fibonacci n} {
  if (same n 0)
    {0}
    {if (same n 1)
      {1}
      {add
        (fibonacci (strain n 1))
        (fibonacci (strain n 2))
      }
    }
})

(say "f0" (fibonacci 0))
(say "f1" (fibonacci 1))
